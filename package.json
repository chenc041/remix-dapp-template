    "@web3modal/ethereum": "^2.2.1",
    "axios": "^1.6.8",
    "ethers": "v6",
    "isbot": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "wagmi": "^2.5.12"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@remix-run/dev": "^2.0.0",
    "@remix-run/eslint-config": "^2.0.0",
    "@types/node": "^20.12.11",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.14",
    "cross-env": "^7.0.3",
    "eslint": "^9.0.0",
    "npm-run-all2": "^6.0.0",
    "rimraf": "^5.0.5",
